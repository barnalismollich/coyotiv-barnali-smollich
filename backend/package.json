    "http-errors": "~1.6.3",
    "livereload": "^0.9.3",
    "mongoose": "^6.3.2",
    "mongoose-autopopulate": "^0.16.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.5.3",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^7.0.0",
    "pug": "2.0.0-beta11",
    "socket.io": "^4.5.1"
