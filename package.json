  "version": "0.0.1",
  "description": "In-class examples for node and Javascript",
  "main": "index_barnali.js",
  "scripts": {
    "lint": "lint-staged",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1",
