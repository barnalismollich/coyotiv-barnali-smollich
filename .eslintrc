{
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier", "jest"],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "prettier/prettier": "error",
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off",
    "eqeqeq": "off",
    "class-methods-use-this": "off"
  }
}
